@model GraffitiVR.Model.Sketch
@{
    Layout = "_Empty";
}
<script src="~/js/aframe.min.js"></script>
<script src="~/js/aframe-environment-component.min.js"></script>
<script src="~/js/bot_aframe.js"></script>
<script>
AFRAME.registerComponent('botdraw', {
  schema: {type: 'string'},
  init: function () {
	@Html.Raw(ViewBag.Code)
  }
});
</script>
<a-scene botdraw="true">
<a-entity light="color: #white; intensity: 1.5" position="-10 10 -10"></a-entity>
<a-entity environment="preset: forest"></a-entity>
</a-scene>




